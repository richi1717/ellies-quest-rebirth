{
  "transform": {
    ".*": "../node_modules/babel-jest"
  },
  "collectCoverageFrom": [
    "client/js/**/*.js"
  ],
  "coverageDirectory": "../coverage/client",
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/client/js/index.js"
  ],
  "setupTestFrameworkScriptFile": "../node_modules/jest-enzyme/lib/index.js",
  "testMatch": ["**client/tests/**/*.test.js"]
}