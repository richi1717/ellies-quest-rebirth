{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React"
    }
  },
  "globals": {
    "before": false,
    "after": false,
    "beforeEach": false,
    "beforeAll": false,
    "describe": false,
    "it": false,
    "test": false,
    "afterEach": false,
    "afterAll": false,
    "browser": false,
    "jest": false,
    "expect": false
  },
  "extends": "airbnb",
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "consistent-return": "warn",
    "eol-last": "warn",
    "import/no-named-as-default-member": "warn",
    "jsx-a11y/href-no-hash": "off",
    "max-len": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-plusplus": "warn",
    "no-prototype-builtins": "warn",
    "no-restricted-syntax": "warn",
    "no-unused-expressions": "off",
    "prefer-arrow-callback": "warn",
    "react/jsx-filename-extension": "off",
    "react/jsx-no-target-blank": "off"
  }
}
